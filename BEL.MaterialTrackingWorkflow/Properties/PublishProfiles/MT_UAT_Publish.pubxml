      <Scope>web.config$</Scope>
      <Match>/configuration/appSettings/add[@key='ClientSecret']/@value</Match>
      <Description>Value for ClientSecret here.</Description>
      <DefaultValue>Mz3cA7F8yUuH4bdWGF/70diA45nzQNPT2xjhleKHgJI=</DefaultValue>
      <Value>Mz3cA7F8yUuH4bdWGF/70diA45nzQNPT2xjhleKHgJI=</Value>
      <Tags>applicationSettings</Tags>
    </MsDeployDeclareParameters>
  </ItemGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String" />
  </ItemGroup>
</Project>